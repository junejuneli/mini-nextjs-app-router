{
  "name": "mini-nextjs-app-router",
  "version": "1.0.0",
  "description": "A minimal implementation of Next.js App Router with RSC Flight Protocol",
  "type": "module",
  "scripts": {
    "build": "tsx build/index.js",
    "start": "tsx server/index.js",
    "dev": "npm run build && npm start",
    "clean": "rm -rf .next",
    "typecheck": "tsc --noEmit",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [
    "next.js",
    "react",
    "rsc",
    "app-router",
    "flight-protocol",
    "streaming",
    "server-components",
    "educational"
  ],
  "author": "junejuneli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junejuneli/mini-nextjs-app-router.git"
  },
  "bugs": {
    "url": "https://github.com/junejuneli/mini-nextjs-app-router/issues"
  },
  "homepage": "https://github.com/junejuneli/mini-nextjs-app-router#readme",
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  }
}
